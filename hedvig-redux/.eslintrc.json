{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:jest/recommended", "prettier"],
  "plugins": ["jest"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest/globals": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "error"
  }
}
